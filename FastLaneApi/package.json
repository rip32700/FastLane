{
  "name": "fastlaneapi",
  "version": "0.0.0",
  "private": true,
  "description": "FastLane Api",
  "author": "Philipp Rieger",
  "scripts": {
    "start": "npm run watch-ts && npm run tslint",
    "build": "tsc",
    "watch-ts": "tsc-watch --onSuccess \"nodemon dist/Main.js\"",
    "test": "mocha --compilers ts:ts-node/register --exit",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/Main.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "@types/async": "^2.0.40",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "^1.16.2",
    "@types/chai": "^4.0.4",
    "@types/chai-http": "^3.0.3",
    "@types/connect-mongo": "0.0.32",
    "@types/cors": "^2.8.1",
    "@types/dotenv": "^2.0.20",
    "@types/errorhandler": "0.0.30",
    "@types/express": "^4.0.35",
    "@types/express-jwt": "0.0.37",
    "@types/express-session": "0.0.32",
    "@types/jquery": "^2.0.41",
    "@types/lodash": "^4.14.63",
    "@types/mocha": "^2.2.44",
    "@types/mongodb": "^2.1.43",
    "@types/mongoose": "^4.7.9",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.12",
    "@types/nodemailer": "^1.3.32",
    "@types/passport": "^0.3.3",
    "@types/passport-facebook": "^2.1.3",
    "@types/passport-local": "^1.0.31",
    "@types/supertest": "^2.0.0",
    "@types/winston": "^2.3.7",
    "async": "^2.1.2",
    "axios": "^0.17.1",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.15.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "concurrently": "^3.4.0",
    "connect-mongo": "^1.3.2",
    "cors": "^2.8.4",
    "dotenv": "^2.0.0",
    "errorhandler": "^1.4.3",
    "express": "^4.14.0",
    "express-jwt": "^5.3.0",
    "express-session": "^1.14.2",
    "express-validator": "^3.1.3",
    "mocha": "^4.0.1",
    "mongoose": "^4.6.6",
    "morgan": "^1.7.0",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "shelljs": "^0.7.7",
    "supertest": "^2.0.1",
    "ts-node": "^3.3.0",
    "tsc-watch": "^1.0.10",
    "tslint": "^5.0.0",
    "typescript": "^2.4.0",
    "winston": "^2.4.0",
    "xss-filters": "^1.2.7"
  }
}
